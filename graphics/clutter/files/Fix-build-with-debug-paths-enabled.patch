From 6295b1571720f9e8f1b8d935f59c9ec8e538752d Mon Sep 17 00:00:00 2001
From: Emmanuele Bassi <ebassi@gnome.org>
Date: Tue, 26 Mar 2013 22:25:37 +0000
Subject: [PATCH] Fix build with debug paths enabled

---
 clutter/clutter-script-parser.c   | 4 ++--
 clutter/x11/clutter-backend-x11.c | 6 +++---
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/clutter/clutter-script-parser.c b/clutter/clutter-script-parser.c
index e424d29..3ed900c 100644
--- clutter/clutter-script-parser.c
+++ clutter/clutter-script-parser.c
@@ -1857,7 +1857,7 @@ apply_layout_properties (ClutterScript    *script,
       CLUTTER_NOTE (SCRIPT,
                     "Setting %s layout property '%s' (type:%s) to "
                     "object '%s' (id:%s)",
-                    set_custom_property ? "custom" : "regular",
+                    iface->set_custom_property != NULL ? "custom" : "regular",
                     name,
                     g_type_name (G_VALUE_TYPE (&value)),
                     g_type_name (oinfo->gtype),
@@ -1956,7 +1956,7 @@ apply_child_properties (ClutterScript    *script,
       CLUTTER_NOTE (SCRIPT,
                     "Setting %s child property '%s' (type:%s) to "
                     "object '%s' (id:%s)",
-                    set_custom_property ? "custom" : "regular",
+                    iface->set_custom_property != NULL ? "custom" : "regular",
                     name,
                     g_type_name (G_VALUE_TYPE (&value)),
                     g_type_name (oinfo->gtype),
diff --git a/clutter/x11/clutter-backend-x11.c b/clutter/x11/clutter-backend-x11.c
index cff80ff..b7cfcc4 100644
--- clutter/x11/clutter-backend-x11.c
+++ clutter/x11/clutter-backend-x11.c
@@ -786,9 +786,9 @@ clutter_backend_x11_create_stage (ClutterBackend  *backend,
   _clutter_backend_add_event_translator (backend, translator);
 
   CLUTTER_NOTE (MISC, "X11 stage created (display:%p, screen:%d, root:%u)",
-                CLUTTER_BACKEND_X11 (backend_x11)->xdpy,
-                CLUTTER_BACKEND_X11 (backend_x11)->xscreen_num,
-                (unsigned int) CLUTTER_BACKEND_X11 (backend_x11)->xwin_root);
+                CLUTTER_BACKEND_X11 (backend)->xdpy,
+                CLUTTER_BACKEND_X11 (backend)->xscreen_num,
+                (unsigned int) CLUTTER_BACKEND_X11 (backend)->xwin_root);
 
   return stage;
 }
-- 
